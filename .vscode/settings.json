{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.associations": {
    ".huskyrc": "json",
    ".lintstagedrc": "json",
    ".prettierrc": "json",
    ".eslintrc": "json",
    ".firebaserc": "json"
  },
  "markdown.extension.toc.updateOnSave": true,
  "markdown.extension.toc.slugifyMode": "github",
  "typescript.tsdk": "node_modules/typescript/lib"
}
